div.total{
  width: 400px;
  text-align:right;
}

@import 'bootstrap.less';

div.item{
  width: 400px;
  max-height: 50px;
  border-style:solid;
border-width:1px;
margin-bottom: 2px;
padding: 3px;
}
div.cart_num{
 vertical-align: bottom;
}

div.block{
  overflow: hidden;
  float: left;
}
div.block label{
  width:160px;
  display:block;
  float:left;
  text-align:left;
}
.ui-datepicker-calendar {
    display: none;
 }
 
 .ui-datepicker-buttonpane button.ui-datepicker-current {
	visibility: hidden;
 }
 
.ui-datepicker-prev,
.ui-datepicker-next {
	display: none;
}

#ui-datepicker-div {
	margin: 3px;
	padding: 3px;
	border-style: solid;
	border-width: 1px;
}

.error {
	/* color: FF0000; */
}

.container {
	max-width: 100%;
	width: 100%;
	padding-left: 15%;
	padding-right: 15%;
	background-color:#EEEEEE;
}

.header {
	height: 50px;
}

.header-left {
	font-size: 250%;
	margin: 0px;
	padding:0px;
}

.header-left a {
	background-color: white;
	color: black;
	border-color: #DDDDDD;
}

.header-left a:hover {
	background-color: #DFDFDF;
	border-color: #DDDDDD;
	color: black;
}

.header-right {
	margin-top: 0px; 
	margin-bottom: 0px; 
	/* border-right: 3px solid #AAAAAA; */
	height: 40px; 
	text-align: right; 
	padding-right: 10px;
}

.body {
	width: 100%;
	background-color:#FFFFFF;
	padding: 30px;
	border: 1px solid #DDDDDD;
	
}

body {
	background-color:#EEEEEE;
}




